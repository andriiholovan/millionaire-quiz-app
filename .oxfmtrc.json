{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "semi": false,
  "singleQuote": true,
  "sortImports": {
    "newlinesBetween": false,
    "groups": [
      "value-external",
      "import",
      { "newlinesBetween": true },
      "value-style"
    ]
  },
  "sortPackageJson": {
    "sortScripts": true
  }
}
